{
  "sourceMaps": true,
  "plugins": [
    "transform-react-jsx",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-object-assign",
    ["babel-plugin-transform-builtin-extend", {
      globals: ["Error", "Array"],
      approximate: true
    }]
  ],
  "presets": [
    "es2015"
  ]
}

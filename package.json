    "node-uuid": "^1.4.7",
    "postcss": "5.0.16",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-bem-helper": "^1.1.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-effects": "^0.4.3",
    "redux-effects-fetch": "^0.5.5",
    "redux-multi": "^0.1.12",
    "redux-thunk": "^2.1.0",
    "request-promise": "^2.0.0",
    "sass-loader": "^3.2.0",
    "semver": "5.1.0",
    "shelljs": "^0.5.3",
    "style-loader": "^0.13.1",
    "styles-toolkit": "github:notonthehighstreet/styles-toolkit",
    "svg-inline-loader": "^0.6.1",
    "toga-loader": "file:packages/toga-loader",

    "codeclimate-test-reporter": "^0.3.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.4.0",
    "eslint-plugin-mocha": "2.0.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^7.2.2",
    "mocha": "^2.2.5",
    "mockery": "1.4.1",
    "nightwatch": "^0.9.3",
    "nodemon": "^1.8.1",
    "nyc": "^6.0.0",
    "pre-commit": "1.1.2",
    "react-addons-test-utils": "^15.3.0",
    "rewire": "^2.5.1",
    "selenium-standalone": "^5.4.1",
    "sinon": "1.17.3",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0"
  }
}

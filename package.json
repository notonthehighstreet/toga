{
  "name": "noths-frontend",
  "version": "1.0.0",
  "description": "A modular set of components and pages for the Notonthehighstreet frontend which are exposed as either raw assets or pre-rendered.",
  "main": "index.js",
  "repository": "https://github.com/notonthehighstreet/noths-frontend",
  "engine": "node >= 4.2.3",
  "scripts": {
    "dev": "env DEBUG=toga,toga:* node_modules/nodemon/bin/nodemon.js --debug ./script/startDev.js",
    "start": "./script/start.js",
    "test": "./script/test.js",
    "create-bundle-indexes": "env DEBUG=toga:* node ./script/createBundleIndexes.js",
    "lint": "./node_modules/eslint/bin/eslint.js ."
  },
  "babel": {
    "sourceMaps": "inline",
    "plugins": [
      "transform-react-jsx"
    ],
    "presets": [
      "es2015"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^1.5.0",
    "autoprefixer": "6.3.3",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "debug": "^2.2.0",
    "es6-promisify": "^3.0.0",
    "express": "^4.13.3",
    "ioredis": "^1.15.0",
    "json-loader": "^0.5.4",
    "lodash": "^3.10.1",
    "memjs": "^0.9.0",
    "memory-fs": "latest",
    "node-sass": "^3.4.2",
    "postcss": "5.0.16",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "request-promise": "^2.0.0",
    "semver": "5.1.0",
    "shelljs": "^0.5.3",
    "toga-component": "file:packages/toga-component",
    "toga-loader": "file:packages/toga-loader",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "browser-sync": "2.11.1",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.15.0",
    "glob": "^6.0.4",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "pre-commit": "1.1.2",
    "react-addons-test-utils": "^0.14.6",
    "rewire": "^2.5.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.1.0",
    "yargs": "^3.32.0"
  }
}

{
  "name": "noths-frontend",
  "version": "1.0.0",
  "description": "A modular set of components and pages for the Notonthehighstreet frontend which are exposed as either raw assets or pre-rendered.",
  "main": "index.js",
  "repository": "https://github.com/notonthehighstreet/noths-frontend",
  "engines": {
    "node": "^6.2.2",
    "npm": "^3.10.3"
  },
  "scripts": {
    "example": "node ./example",
    "start": "DEBUG=toga,toga:* ./script/start/index.js --config ./app/config/application.json",
    "dev": "DEBUG=toga,toga:* nodemon ./script/start/index.js -- --config ./app/config/application.json --config ./app/config/devOverrides.json",
    "precache": "DEBUG=toga,toga:* ./script/preCache.js --config ./app/config/application.json --config ./app/config/devOverrides.json",
    "test": "npm run test-unit",
    "coverage": "nyc --require babel-core/register --all npm test",
    "codeclimate": "npm run coverage -- --reporter=lcov --reporter=text-lcov | CODECLIMATE_REPO_TOKEN=7072dea161cbd60d39bea5b16f7e0f5c502f16da4627893def96b2efebe0b9e3 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test-unit": "NODE_ENV=production NODE_PATH=. _mocha --require ./tests/spec/specSetup.js --opts ./tests/config/mocha.opts '{tests/spec,components}/**/*.spec.js'",
    "test-integration": "_mocha -t 7000 --opts ./tests/config/mocha.opts 'tests/integration/**/*.spec.js' --config ./app/config/application.json --config './app/config/integrationOverrides.json'",
    "test-e2e": "npm run nightwatch -- --env local",
    "test-e2e-xb": "npm run nightwatch -- --env chrome_win",
    "prenightwatch": "selenium-standalone install --version=2.53.1",
    "nightwatch": "nightwatch -o ./tests/e2e/tests_output -c ./tests/config/nightwatch.conf.js"
  },
  "pre-commit": [
    "lint",
    "coverage"
  ],
  "nyc": {
    "include": [
      "app/**/*.js",
      "components/**/*.js"
    ],
    "exclude": [
      "components/test*/**/*",
      "**/*/*.spec.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^1.5.0",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "breadboard": "^7.0.0",
    "bunyan": "^1.7.1",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "debug": "2.2.0",
    "deep-assign": "^2.0.0",
    "es6-promise": "^3.2.1",
    "es6-promisify": "^3.0.0",
    "express": "^4.13.3",
    "express-domain-middleware": "0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^6.0.4",
    "hash-files": "^1.1.1",
    "honeybadger": "0.9.1",
    "ioredis": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.12",
    "lodash": "^3.10.1",
    "memory-fs": "0.3.0",
    "newrelic": "^1.25.4",
